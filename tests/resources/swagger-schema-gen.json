{
    "title": "A JSON Schema for Swagger 2.0 API.",
    "id": "http://swagger.io/v2/schema.json#",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "swagger",
        "info",
        "paths"
    ],
    "additionalProperties": false,
    "patternProperties": {
        "^x-": {
            "$ref": "#/definitions/vendorExtension"
        }
    },
    "properties": {
        "swagger": {
            "type": "string",
            "enum": [
                "2.0"
            ],
            "description": "The Swagger version of this document."
        },
        "info": {
            "$ref": "#/definitions/info"
        },
        "host": {
            "type": "string",
            "pattern": "^[^{}/ :\\\\]+(?::\\d+)?$",
            "description": "The host (name or ip) of the API. Example: \\'swagger.io\\'"
        },
        "basePath": {
            "type": "string",
            "pattern": "^/",
            "description": "The base path to the API. Example: \\'/api\\'."
        },
        "schemes": {
            "$ref": "#/definitions/schemesList"
        },
        "consumes": {
            "description": "A list of MIME types accepted by the API.",
            "allOf": [
                {
                    "$ref": "#/definitions/mediaTypeList"
                }
            ]
        },
        "produces": {
            "description": "A list of MIME types the API can produce.",
            "allOf": [
                {
                    "$ref": "#/definitions/mediaTypeList"
                }
            ]
        },
        "paths": {
            "$ref": "#/definitions/paths"
        },
        "definitions": {
            "$ref": "#/definitions/definitions"
        },
        "parameters": {
            "$ref": "#/definitions/parameterDefinitions"
        },
        "responses": {
            "$ref": "#/definitions/responseDefinitions"
        },
        "security": {
            "$ref": "#/definitions/security"
        },
        "securityDefinitions": {
            "$ref": "#/definitions/securityDefinitions"
        },
        "tags": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/tag"
            },
            "uniqueItems": true
        },
        "externalDocs": {
            "$ref": "#/definitions/externalDocs"
        }
    },
    "definitions": {
        "info": {
            "type": "object",
            "description": "General information about the API.",
            "required": [
                "version",
                "title"
            ],
            "additionalProperties": false,
            "patternProperties": {
                "^x-": {
                    "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\ContactPatternPropertiesPattern"
                }
            },
            "properties": {
                "title": {
                    "type": "string",
                    "description": "A unique and precise title of the API."
                },
                "version": {
                    "type": "string",
                    "description": "A semantic version number of the API."
                },
                "description": {
                    "type": "string",
                    "description": "A longer description of the API. Should be different from the title.  GitHub Flavored Markdown is allowed."
                },
                "termsOfService": {
                    "type": "string",
                    "description": "The terms of service for the API."
                },
                "contact": {
                    "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\Contact"
                },
                "license": {
                    "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\License"
                }
            }
        },
        "paths": {
            "type": "object",
            "description": "Relative paths to the individual endpoints. They must be relative to the \\'basePath\\'.",
            "patternProperties": {
                "^x-": {
                    "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\ContactPatternPropertiesPattern"
                },
                "^/": {
                    "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\PathItem"
                }
            },
            "additionalProperties": false,
            "properties": {}
        },
        "definitions": {
            "type": "object",
            "additionalProperties": {
                "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\Schema"
            },
            "description": "One or more JSON objects describing the schemas being consumed and produced by the API.",
            "properties": {}
        },
        "parameterDefinitions": {
            "type": "object",
            "additionalProperties": {
                "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\OperationParametersItemsOneOf0"
            },
            "description": "One or more JSON representations for parameters",
            "properties": {}
        },
        "responseDefinitions": {
            "type": "object",
            "additionalProperties": {
                "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\Response"
            },
            "description": "One or more JSON representations for parameters",
            "properties": {}
        },
        "externalDocs": {
            "type": "object",
            "additionalProperties": false,
            "description": "information about external documentation",
            "required": [
                "url"
            ],
            "properties": {
                "description": {
                    "type": "string"
                },
                "url": {
                    "type": "string",
                    "format": "uri"
                }
            },
            "patternProperties": {
                "^x-": {
                    "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\ContactPatternPropertiesPattern"
                }
            }
        },
        "vendorExtension": {
            "description": "Any property starting with x- is valid.",
            "properties": {}
        },
        "security": {
            "type": "array",
            "items": {
                "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\OperationSecurityItems"
            },
            "uniqueItems": true,
            "properties": {}
        },
        "tag": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "name"
            ],
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "externalDocs": {
                    "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\ExternalDocs"
                }
            },
            "patternProperties": {
                "^x-": {
                    "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\ContactPatternPropertiesPattern"
                }
            }
        },
        "securityDefinitions": {
            "type": "object",
            "additionalProperties": {
                "oneOf": [
                    {
                        "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\BasicAuthenticationSecurity"
                    },
                    {
                        "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\ApiKeySecurity"
                    },
                    {
                        "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\Oauth2ImplicitSecurity"
                    },
                    {
                        "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\Oauth2PasswordSecurity"
                    },
                    {
                        "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\Oauth2ApplicationSecurity"
                    },
                    {
                        "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\Oauth2AccessCodeSecurity"
                    }
                ]
            },
            "properties": {}
        },
        "mediaTypeList": {
            "type": "array",
            "items": {
                "objectItemClass": "Swaggest\\PhpCodeBuilder\\Tests\\Tmp\\Swagger\\ConsumesAllOf0Items"
            },
            "uniqueItems": true,
            "properties": {}
        },
        "schemesList": {
            "type": "array",
            "description": "The transfer protocol of the API.",
            "items": {
                "type": "string",
                "enum": [
                    "http",
                    "https",
                    "ws",
                    "wss"
                ]
            },
            "uniqueItems": true,
            "properties": {}
        }
    }
}